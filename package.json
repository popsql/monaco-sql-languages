    "jest": "^29.5.0",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "monaco-editor": "^0.38.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "typescript": "^4.9"
